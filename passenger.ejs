<div class = "passengerBox" id ="pass<%=parseInt(i)+1%>">
	<h3>PASSENGER <%=passengerNumber%></h3>
	<div class="lidiv">
		<label class="lab" for="fnamein">First Name</label>
		<input class="inp" type="text" name="fnamein" form="passengerform"/>
	</div>
	<div class="lidiv" >
		<label class="lab" for="lnamein">Last Name</label>
		<input class="inp" type="text"  name="lnamein" form="passengerform"/>
	</div>
	<div class="lidiv" >
		<label class="lab" for="dobin">Date Of Birth</label>
		<input class="inp" type="date"  name="dobin" form="passengerform"/>
	</div>
	<div class="lidiv" >
		<label class="lab" for="phonein">Phone #</label>
		<input class="inp" type="text"  name="phonein" form="passengerform"/>
	</div>
	<div class="lidiv" >
		<label class="lab" for="genderin">Gender</label>
		<select class="inp"  name="gender">
			<option value="male">Male</option>
			<option value="female">Female</option>
			<option value="other">Other</option>
		</select>
	</div>
</div>

